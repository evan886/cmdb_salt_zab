#install py3.6
wget -c https://mirrors.huaweicloud.com/python/3.6.0/Python-3.6.0.tar.xz
   tar xvf Python-3.6.0.tar.xz 
   cd Python-3.6.0/
   ./configure --prefix=/usr/local/python3.6
   make  -j7
   sudo make install 
sudo ln -s /usr/local/python3.6/bin/python3.6 /usr/bin/python3.6

#开始配置
virtualenv -p /usr/bin/python3.6 venv 
 source  venv/bin/activate

python3.6 -m pip install -i https://mirrors.aliyun.com/pypi/simple    -r requirements.txt
 python3.6  -m pip install pymysql

python -m django --version
